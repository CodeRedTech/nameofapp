<% if signed_in? %>
  <div class="pull-right">
    <%= link_to 'New Product', new_product_path, class: "btn btn-default" %>
  </div>
  <div class="clearfix"></div>
<% end %>

<h1>Products</h1>


<div class="container">
  <div class="row">
    <% cache(cache_key_for_products) do |product| %>
      <%= render partial: "product", collection: @products %>
    <% end %>
  </div><!-- /col -->
</div>


